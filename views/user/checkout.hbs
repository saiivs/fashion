
    <!-- Breadcrumb Section Begin -->
    <section class="breadcrumb-option">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="breadcrumb__text">
                        <h4>Check Out</h4>
                        <div class="breadcrumb__links">
                            <a href="./index.html">Home</a>
                            <a href="./shop.html">Shop</a>
                            <span>Check Out</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Breadcrumb Section End -->

    <!-- Checkout Section Begin -->
    <section class="checkout spad">
        <div class="container">
            <div class="checkout__form">
                
                <form action="" id="checkOut">
                    <div class="row">
                        <div class="col-lg-7 col-md-6">
                            {{#if Address}}
                            <button class="btn btn-dark coupon__code py-1 px-2" type="button" data-toggle="collapse"
                                data-target="#collapseExample" aria-expanded="false" aria-controls="collapseExample">
                                Saved Address
                            </button>
                            {{/if}}
                            <div class="collapse"  id="collapseExample">
                                <div class="card card-body" style="background-color: #f3f2ee;">
                                    
                                        <p class="font-weight-bold">Select One Address:</p>

                                        {{#each Address}}
                                        <input style="width: 11px;" type="radio" id="age1" name="radio" value="{{this._id}}">
                                        <label  for="age2">{{this.name}}<br>{{this.country}},{{this.STaddress}}<br>{{this.APaddress}},{{this.Town_city}}<br>{{this.email}},{{this.phone}}</label><br>
                                        <hr>
                                        {{/each}}
                                        {{!-- <input type="radio" id="age2" name="age" value="60">
                                        <label for="age2">31 - 60</label><br>
                                        <input type="radio" id="age3" name="age" value="100">
                                        <label for="age3">61 - 100</label><br><br> --}}
                                        
                                    
                                </div>
                            </div>
                            <h6 class="checkout__title">Billing Details</h6>
                            <input type="radio" id="billing" name="radio" value="true" checked>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="checkout__input">
                                        <p>Fist Name<span>*</span><span class="text-danger" id="ERR_1"></span></p>
                                        <input type="text" id="f1" class="text-dark" name="name" value="{{radio.name}}" onkeyup="validName()">
                                        <input type="text" name="user" value="{{acc._id}}" hidden>

                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="checkout__input">
                                        <p>Last Name<span>*</span><span class="text-danger" id="ERR_2"></span></p>
                                        <input type="text" id="f2" class="text-dark"  name="lastName"
                                            onkeyup="validLname()">

                                    </div>
                                </div>
                            </div>
                            <div class="checkout__input">
                                <p>Country<span>*</span><span class="text-danger" id="ERR_3"></span></p>
                                <input type="text" id="f3" name="country"  class="text-dark" onkeyup="validCountry()">

                            </div>
                            <div class="checkout__input">
                                <p>Address<span>*</span><span class="text-danger" id="ERR_4"></span></p>
                                <input type="text" placeholder="Street Address"  class="text-dark" id="f4"
                                    onkeyup="validStreet()" name="STaddress" class="checkout__input__add">
                                <span class="text-danger" id="ERR_5"></span>
                                <input type="text" name="APaddress"  class="text-dark" id="f5" onkeyup="validAP()"
                                    placeholder="Apartment, suite, unite ect (optinal)">

                            </div>
                            <div class="checkout__input">
                                <p>Town/City<span>*</span><span class="text-danger" id="ERR_6"></span></p>
                                <input type="text" id="f6" class="text-dark"  onkeyup="validCity()" name="Town_city">

                            </div>
                            <div class="checkout__input">
                                <p>Country/State<span>*</span><span class="text-danger" id="ERR_7"></span></p>
                                <input type="text" id="f7" class="text-dark"  onkeyup="validState()"
                                    name="Country_state">

                            </div>
                            <div class="checkout__input">
                                <p>Postcode / ZIP<span>*</span><span class="text-danger" id="ERR_8"></span></p>
                                <input type="number" id="f8" class="text-dark"  onkeyup="validPost()" name="postCode">

                            </div>
                            <div class="row">
                                <div class="col-lg-6">
                                    <div class="checkout__input">
                                        <p>Phone<span>*</span><span class="text-danger" id="ERR_9"></span></p>
                                        <input type="number" class="text-dark" id="f9"  onkeyup="validPhone()"
                                            name="phone">

                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div class="checkout__input">
                                        <p>Email<span>*</span><span class="text-danger" id="ERR_10"></span></p>
                                        <input type="text" id="f10"  onkeyup="validEmail()" name="email">

                                    </div>
                                </div>
                                <div class="col-lg-6">
                                    <div>
                                        <input type="checkbox" id="html" name="savedaddress" value="save">
                                        <label for="html">Save Address</label><br>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4 col-md-6">
                            <div class="checkout__order">
                                <h4 class="order__title">Your order</h4>
                                <div class="checkout__order__products">Product <span>Total</span></div>
                                <ul class="checkout__total__products">
                                    {{#each products}}
                                    <li>{{inc @index}} {{this.product.name}}<span>{{this.total}}</span></li>
                                    {{!-- <li>02. German chocolate <span>$ 170.0</span></li>
                                    <li>03. Sweet autumn <span>$ 170.0</span></li>
                                    <li>04. Cluten free mini dozen <span>$ 110.0</span></li> --}}
                                    {{/each}}
                                </ul>
                                <ul class="checkout__total__all">
                                    <li>Total <span>{{preDeduction}}</span></li>
                                    <li>Offer <span>{{offer}}</span><span>-</span></li>
                                    <li>Coupon <span>{{value}}</span><span>-</span></li>
                                    <li>Subtotal <span>{{total}}</span></li>
                                    
                                    {{!-- <li>Total <span>$750.99</span></li> --}}
                                </ul>
                                <div class="checkout__input__checkbox">
                                    <label for="acc-or">
                                        Create an account?
                                        <input type="checkbox" id="acc-or">
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                                <p>Choose your payment method.
                                </p>
                                <div class="checkout__input__checkbox">
                                    <label for="payment">
                                        COD
                                        <input name="paymentMethod" type="radio" value="COD" id="payment" checked>
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                                <div class="checkout__input__checkbox">
                                    <label for="paypal">
                                        Online Payment
                                        <input name="paymentMethod" type="radio" value="Razor" id="paypal">
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                                <div class="checkout__input__checkbox">
                                    <label for="p">
                                        PayPal
                                        <input name="paymentMethod" type="radio" value="paypal" id="p">
                                        <span class="checkmark"></span>
                                    </label>
                                </div>
                                <button type="submit" onclick="return valid()" class="site-btn">PLACE ORDER</button>
                           
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </section>
    <!-- Checkout Section End -->

