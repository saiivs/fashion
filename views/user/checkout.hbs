    <script>
        function validName() {
            let val = document.getElementById('f1').value
            let mess = document.getElementById('ERR_1')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^[a-zA-Z]*$/)) {
                mess.innerHTML = 'Only Alphabets Are Allowed'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function validLname() {
            let val = document.getElementById('f2').value
            let mess = document.getElementById('ERR_2')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^[a-zA-Z]*$/)) {
                mess.innerHTML = 'Only Alphabets Are Allowed'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function validCountry() {
            let val = document.getElementById('f3').value
            let mess = document.getElementById('ERR_3')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^[a-zA-Z]*$/)) {
                mess.innerHTML = 'Only Alphabets Are Allowed'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function validStreet() {
            let val = document.getElementById('f4').value
            let mess = document.getElementById('ERR_4')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^[a-zA-Z]*$/)) {
                mess.innerHTML = 'Only Alphabets Are Allowed'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function validAP() {
            let val = document.getElementById('f5').value
            let mess = document.getElementById('ERR_5')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^[a-zA-Z]*$/)) {
                mess.innerHTML = 'Only Alphabets Are Allowed'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }

        function validState() {
            let val = document.getElementById('f7').value
            let mess = document.getElementById('ERR_7')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^[a-zA-Z]*$/)) {
                mess.innerHTML = 'Only Alphabets Are Allowed'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function validPost() {
            let val = document.getElementById('f8').value
            let mess = document.getElementById('ERR_8')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^[0-9]/)) {
                mess.innerHTML = 'Only Numbers'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function validPhone() {
            let val = document.getElementById('f9').value
            let mess = document.getElementById('ERR_9')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^\d{10}$/)) {
                mess.innerHTML = 'Enter Valid Number'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function validEmail() {
            let val = document.getElementById('f10').value
            let mess = document.getElementById('ERR_10')
            if (val == '') {
                mess.innerHTML = "Fill the Fields"
                return false;
            }
            if (!val.match(/^\w+([\.-]?\w+)@\w+([\.-]?\w+)(\.\w{2,3})+$/)) {
                mess.innerHTML = 'Enter Valid Email'
                return false;
            }
            mess.innerHTML = ""
            return true;
        }
        function valid() {
            let billing=document.getElementById('billing')
            if(!billing.checked){
                return true
            }
            if (!validName() || !validLname() || !validCountry() || !validCountry() || !validStreet() || !validAP() || !validState() || !validPost() || !validPhone() || !validEmail()) {
                return false;
            }
            
            else {
                return true;
            }
        }
    </script>

